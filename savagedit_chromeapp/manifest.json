{
  "name": "SAVAGEdit",
  "description": "A fast, web-based, Javascript-driven SVG editor that works in any modern browser",
  "manifest_version": 2,
  "version": "1.0.0.",
  "app": {
    "urls": [
      "*://savagedit.com"
    ],
    "launch": {
      "web_url": "http://savagedit.com"
    }
  },
  "icons": {
    "128": "icon_128.png"
  },
  "permissions": [
    "unlimitedStorage",
    "notifications"
  ]
}
